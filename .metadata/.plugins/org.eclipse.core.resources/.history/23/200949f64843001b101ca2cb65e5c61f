package task1;

public class ReverseCharacter 
{
	public static void main(String[] args) 
	{
		String s = "asd$mkmvd&od##m";
		
		char[] arr = s.toCharArray();
		
		for (int i = 0; i < arr.length; i++) 
		{
			if ((arr[i]>='a' && arr[i]<='z') || (arr[i]>='A' && arr[i]<='Z'))
			{
				System.out.print(arr[i]);
			}
		}
		
		
	}
}
