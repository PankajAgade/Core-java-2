package task1;

public class Re 
{
	static int i = 1;
	static int j = 1;
	static int imax = 5;
	static int jmax = 5;
	
	public static void main(String[] args) 
	{
		print();
	}

	private static void print()
	{
		System.out.println((i)+" "+(j));
		
		if (i<=imax && j<=jmax)
		{
			if (j==jmax)
			{
				i++;
				j=1;
			}
			j++;
			print();
		}
	}	
}
