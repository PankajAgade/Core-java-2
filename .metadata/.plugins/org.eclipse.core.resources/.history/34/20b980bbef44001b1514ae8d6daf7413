package task1;

public class Re 
{
	static int i = 1;
	static int j = 1;
	static int imax = 5;
	static int jmax = 5;
	
	public static void main(String[] args) 
	{
		print(i,j);
	}

	private static void print(int i2, int j2)
	{
		if (i<=imax && j<=jmax && j2<=jmax)
		{
			
			System.out.println((i)+" "+(j));
			
			if (j==j2)
			{
				i++;
				j=1;
			}
			print(i,++j2);
		}
	}

	
}
