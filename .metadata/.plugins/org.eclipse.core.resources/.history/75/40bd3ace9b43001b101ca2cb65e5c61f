package task1;

public class E12Elements 
{
	public static void main(String[] args) 
	{
		int[] arr = {5,6,3,12,1,2,8,9,10,4,11,7};
		
		for (int i = 0; i < (arr.length/3)/2; i++)
		{
			if (i==0)
			{
				int start = 0;
				int ends = arr.length-3;
				for (int j = start; j <= start+3; j++) 
				{
					System.out.println("l");
				}
			}
		}
	}
}
