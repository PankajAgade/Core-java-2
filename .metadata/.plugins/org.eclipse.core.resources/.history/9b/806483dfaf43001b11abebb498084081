package task1;

public class E12Elements 
{
	public static void main(String[] args) 
	{
		int[] arr = {5,6,3,12,1,2,8,9,10,4,11,7};
		
		for (int i : arr) 
		{
			System.out.print(i+" ");
		}
		System.out.println();
		
		for (int i = 0; i < 4; i++)//(arr.length/3)/2
		{
			
		}
		
		for (int i : arr) 
		{
			System.out.print(i+" ");
		}
		System.out.println();
	}
}
