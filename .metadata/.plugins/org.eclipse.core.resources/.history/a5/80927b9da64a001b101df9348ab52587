package test;

public class Solution8 
{
	public static void main(String[] args) 
	{
		String s = "AAAAAAABBBBCCCCCDDDDDDDDDDEEEEEEEEEFF";
		String s1 = "";
		
		int count=1;
		for (int i = 0; i < s.length(); i++) 
		{
			if(s.charAt(i)==s.charAt(i+1))
			{
				count++;
			}
			else
			{
				s1=s1+s.charAt(i)+count;
			}
		}
	}
}
